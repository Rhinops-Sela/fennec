{
  "auth": {
    "enabled": false
  },
  "master": {
    "persistence": {
      "size": "20Gi"
    },
    "resources": {
      "requests": {
        "memory": "1G",
        "cpu": 1
      }
    },
    "tolerations": [
      {
        "key": "redis",
        "operator": "Equal",
        "value": "true",
        "effect": "NoSchedule"
      }
    ],
    "nodeSelector": {
      "role": "redis"
    }
  },
  "networkPolicy": {
    "enabled": true
  }
}
