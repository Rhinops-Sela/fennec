{
   "clusterName": "elasticsearch",
   "replicas": 1,
   "minimumMasterNodes": 1,
   "persistence": {
      "enabled": true,
      "annotations": {}
   },
   "nodeSelector": {
      "role": "elk"
   },
   "tolerations": [
      {
         "key": "elk",
         "operator": "Equal",
         "value": "true"
      }
   ],
   "ingress": {
      "enabled": false,
      "annotations": {},
      "className": "nginx",
      "pathtype": "ImplementationSpecific",
      "hosts": [
        {
          "host": "HOSTNAME",
          "paths": [
            {
              "path": "/"
            }
          ]
        }
      ],
      "tls": []
    }
}
