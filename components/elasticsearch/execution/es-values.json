{
   "clusterName": "elasticsearch",
   "replicas": 1,
   "minimumMasterNodes": 1,
   "persistence": {
      "enabled": true,
      "annotations": {}
   },
   "nodeSelector": {
      "role": "elk"
   },
   "tolerations": [
      {
         "key": "elk",
         "operator": "Equal",
         "value": "true"
      }
   ],
   "ingress": {
     "enabled":false,
     "hosts": [
       {
         "host": "HOSTNAME",
         "paths": [
           {
             "path": "/"
           }
         ]
       }
     ]
   }
}
