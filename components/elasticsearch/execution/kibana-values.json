{
  "elasticsearchHosts": "http://elasticsearch-master:9200",
  "tolerations": [
    {
      "key": "elk",
      "operator": "Equal",
      "value": "true",
      "effect": "NoSchedule"
    }
  ],
  "nodeSelector": {
    "role": "elk"
  },
  "kibanaConfig": {
    "kibana.yml": "monitoring.enabled: true"
  },
  "service":{
    "port": 80
  },
  "ingress": {
    "enabled":false,
    "hosts": [
      {
        "host": "HOSTNAME",
        "paths": [
          {
            "path": "/"
          }
        ]
      }
    ]
  }
}