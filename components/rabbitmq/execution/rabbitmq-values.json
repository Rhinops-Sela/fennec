{
  "cp-rabbitmq": {
    "podAnnotations": {
      "cluster-autoscaler.kubernetes.io/safe-to-evict": "false"
    },
    "resources": {
      "limits": {
        "cpu": 2,
        "memory": "4Gi"
      },
      "requests": {
        "cpu": 2,
        "memory": "4Gi"
      }
    },
    "imageTag": "3.9.8",
    "enabled": true,
    "brokers": 3,
    "nodeSelector": {
      "role": "rabbitmq"
    },
    "tolerations": [
      {
        "key": "rabbitmq",
        "operator": "Equal",
        "value": "true"
      }
    ],
    "persistence": {
      "enabled": true,
      "size": "25Gi"
    },
    "configurationOverrides": {
      "offsets.topic.replication.factor": "1"
    },
    "heapOptions": "-Xms2g -Xmx2g"
  },
  "cp-control-center": {
    "enabled": false
  },
  "cp-schema-registry": {
    "enabled": false
  },
  "cp-ksql-server": {
    "enabled": false
  },
  "cp-rabbitmq-connect": {
    "enabled": false
  },
  "cp-rabbitmq-rest": {
    "enabled": false
  }
}
