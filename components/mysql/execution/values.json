{
  "primary":{
    "nodeSelector": {
      "role": "mysql"
    },
    "tolerations": [
      {
        "key": "mysql",
        "operator": "Equal",
        "value": "true"
      }
    ],
    "resources":{
      "limits": {
        "cpu": 3,
        "memory": "6Gi"
      },
      "requests": {
        "cpu": 3,
        "memory": "6Gi"
      }
    }
  },
  "metrics": {
    "enabled": true
  },
  "service": {
    "type": "ClusterIP",
    "edgeService": {
      "name": "edge",
      "targetPort": 4566,
      "port": 80
    }
  }
}